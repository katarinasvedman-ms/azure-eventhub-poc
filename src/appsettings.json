{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApiKey": "@Microsoft.KeyVault(SecretUri=https://<your-keyvault>.vault.azure.net/secrets/ApiKey)",
  "EventHub": {
    "FullyQualifiedNamespace": "eventhub-dev-eyeqfiorm5tv2.servicebus.windows.net",
    "HubName": "logs",
    "ConsumerGroup": "logs-consumer",
    "ConnectionString": "@Microsoft.KeyVault(SecretUri=https://<your-keyvault>.vault.azure.net/secrets/EventHubConnectionString)",
    "StorageConnectionString": "@Microsoft.KeyVault(SecretUri=https://<your-keyvault>.vault.azure.net/secrets/StorageConnectionString)",
    "StorageContainerName": "event-hub-checkpoints",
    "UseKeyAuthentication": true,
    "PartitionCount": 24
  },
  "Api": {
    "BatchSize": 1000,
    "BatchTimeoutMs": 5000,
    "MaxConcurrentPartitionProcessing": 10,
    "PartitionAssignmentStrategy": "RoundRobin"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "@Microsoft.KeyVault(SecretUri=https://<your-keyvault>.vault.azure.net/secrets/AppInsightsKey)"
  },
  "ConnectionStrings": {
    "SqlDatabase": "@Microsoft.KeyVault(SecretUri=https://<your-keyvault>.vault.azure.net/secrets/SqlDatabaseConnection)"
  }
}
